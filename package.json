{
  "name": "js-to-sh",
  "description": "Turn javascript code into shellscript",
  "version": "1.1.5",
  "main": "dist/mjs/index.cjs",
  "module": "dist/mjs/index.js",
  "typings": "dist/types/index.d.ts",
  "type": "module",
  "bin": {
    "tjss": "dist/mjs/cli.js",
    "transpiler-js-shell": "dist/mjs/cli.js",
    "js-to-sh": "dist/mjs/cli.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types/index.d.ts"
      ],
      "globals": [
        "./dist/types/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./globals": {
      "import": "./dist/types/types/globals.d.ts",
      "types": "./dist/types/types/globals.d.ts"
    },
    "./loader": {
      "import": "./dist/mjs/loader/index.js",
      "types": "./dist/types/loader/index.d.ts"
    }
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "cli": "tsx src/cli.ts",
    "test": "tsx test/app.ts && chmod 777 output/*",
    "build": "rimraf dist && npm run build:mjs && npm run build:dts && npm run build:cjs && cp -R src/transformers/shellscript dist/mjs/transformers/shellscript && ./fixup.sh && npm run lint -- --fix",
    "build:mjs": "tsc -p tsconfig.mjs.json && tsc-alias -p tsconfig.mjs.json",
    "build:cjs": "npx tsx build.ts",
    "build:dts": "tsc -p tsconfig.dts.json && tsc-alias -p tsconfig.dts.json",
    "lint": "eslint -c eslint.config.js"
  },
  "keywords": [
    "transpiler",
    "javascript",
    "transpiler-js",
    "transpilers",
    "builder",
    "shellscript",
    "js-to-sh",
    "ast",
    "abstract-syntax-tree"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ashu11-A/Transpiler-JS-Shell.git"
  },
  "author": "Ashu11A",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Ashu11-A/Transpiler-JS-Shell/issues"
  },
  "homepage": "https://github.com/Ashu11-A/Transpiler-JS-Shell",
  "dependencies": {
    "@loggings/beta": "^2.0.0",
    "abstract-syntax-tree": "^2.22.0",
    "glob": "^11.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/node": "^22.2.0",
    "eslint": "^9.9.0",
    "globals": "^15.9.0",
    "meriyah": "^5.0.0",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.10",
    "tsup": "^8.2.4",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.1"
  }
}
